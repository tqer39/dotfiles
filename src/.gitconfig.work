	name = tqer39
	email = tqer39@gmail.com
[core]
	editor = code --wait
[push]
	default = simple
	autoSetupRemote = true
[commit]
	gpgsign = false
[alias]
	new-feature-branch = "!f() { \\\n  TITLE=\"$1\"; \\\n  DATE=$(date +%y%m%d); \\\n  git checkout main && \\\n  git fetch origin -p && \\\n  git reset --hard origin/main && \\\n  HASH=$(git rev-parse --short=6 HEAD) && \\\n  git checkout -b \"${TITLE}-${DATE}-${HASH}\" ; \\\n}; f"
